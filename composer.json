{
    "name": "tiper-vendor/tiper-project",
    "description": "A Tiper web project to process Duga API data.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Masakatsu Watanabe",
            "email": "bicstation@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "vlucas/phpdotenv": "^5.0", 
        "phpmailer/phpmailer": "^6.9" 
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
